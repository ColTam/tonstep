/*加载界面*/
QLabel#screenLabel {
    color:#F0F0F0;
}

QProgressBar#screenProgressBar {
    border:none;
    border-radius:5px;
    border-top-left-radius:2px;
    border-top-right-radius:2px;
    text-align:center;
    background:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #282828);
}

QProgressBar#screenProgressBar::chunk {
    border-radius: 5px;
    border-top-left-radius:2px;
    border-top-right-radius:2px;
    background:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #D7801A, stop:1 #F46320);
    margin-bottom: 1px;
}
/*标题栏*/
QLabel#iconLabel {
/*    border-top-left-radius: 5px;*/
    padding-left: 5px;
    padding-right: 3px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #383838);
}

QLabel#titleLabel {
    color: #F0F0F0;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #383838);
}

QToolButton#minimizeButton {
    border: none;
    border-radius: 0px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #383838);
    image: url(:/icon/icon/minimize.png);
}

QToolButton#maximizeButton {
    border: none;
    border-radius: 0px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #383838);
    image: url(:/icon/icon/maximize.png);
}

QToolButton#restoreButton {
    border: none;
    border-radius: 0px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #383838);
    image: url(:/icon/icon/restore.png);
}

QToolButton#minimizeButton:hover, QToolButton#maximizeButton:hover, QToolButton#restoreButton:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #686868, stop:1 #585858);
}

QToolButton#minimizeButton:pressed, QToolButton#maximizeButton:pressed, QToolButton#restoreButton:pressed {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #383838, stop:1 #282828);
}

QToolButton#closeButton {
    border: none;
    border-radius: 0px;
/*    border-top-right-radius: 5px; */
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #383838);
    image: url(:/icon/icon/close.png);
}

QToolButton#closeButton:hover {
    background: #E81123;
}

QToolButton#closeButton:pressed {
    background: #F1707A;
}

