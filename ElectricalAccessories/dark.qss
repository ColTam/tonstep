QPushButton 
{
    border: 1px solid #727272;
    border-bottom-width:1px;
    border-radius: 4px;
    color: #F0F0F0;
    outline: none;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #FF383838, stop:1 #FF282828);
    min-width: 48px;
    min-height: 20px;
}

QPushButton:hover
{
    border-bottom-color: #F46320;
    border-bottom-width: 2px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #FF646464, stop:1 #FF545454);
}

QPushButton:pressed 
{
    border-bottom-color: #727272;
    border-bottom-width: 1px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #FF383838, stop:1 #FF282828);
}

QPushButton:!enabled 
{
    color: #ADADAD;
}

QPushButton#pushButton_6 {
    border: 1px solid #727272;
    border-radius: 0px;
