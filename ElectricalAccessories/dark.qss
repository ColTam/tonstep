QTableWidget {
    border: 1px solid #727272;
    border-left: 2px solid #727272;
    border-radius: 2px;
    selection-color:#F0F0F0;
    selection-background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #383838, stop:1 #282828);
}

QTableWidget::indicator{ 
    width: 14px;  
    height: 14px;   
}  

QTableView::indicator:enabled:unchecked{
    image:url(:/icon/icon/unchecked.png);  
}

QTableView::indicator:enabled:unchecked:hover {
    image:url(:/icon/icon/unchecked_hover.png); 
}

QTableView::indicator:enabled:unchecked:pressed{
    image:url(:/icon/icon/unchecked_pressed.png);  
}

QTableView::indicator:enabled:checked{    
    image:url(:/icon/icon/checked.png); 
}

QTableView::indicator:enabled:checked:hover{    
    image:url(:/icon/icon/checked_hover.png); 
}

QTableView::indicator:enabled:checked:pressed{    
    image:url(:/icon/icon/checked_pressed.png); 
}

QTableWidget::item{
    border:none;   
    border-right: 1px solid #545454;
    border-bottom: 1px solid #545454;
    background: #FBFFFB;
}

QTableWidget::item:hover{
    color:#F0F0F0;
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #646464, stop:1 #545454);
}

QTableWidget::item:selected{
    color:#F0F0F0;
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #383838, stop:1 #282828);
}

QHeaderView::section{
    border:1px solid qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #545454, stop:1 #646464);
    padding: 3px;
    color:#F0F0F0;
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #545454, stop:1 #646464);
}

QScrollBar:vertical {
    width: 13px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #484848, stop:1 #4B4B4B);
    margin: 13px 0 13px 0;
}

QScrollBar::handle:vertical {
    width: 13px;
    min-height: 30px;
    border-radius: 4px;
    margin-left:2px;
    margin-right:2px;
    margin-top: 10px;
    margin-bottom: 10px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #AAAAAA, stop:1 #7B7B7B);
}

QScrollBar::handle:vertical:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #9C9C9C, stop:1 #6D6D6D);
}

QScrollBar::sub-line:vertical {
    height: 13px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #484848, stop:1 #4B4B4B);
    image: url(:/icon/icon/arrow_up.png);
    subcontrol-position: top;
    subcontrol-origin: margin;
    padding-top:5px;
    padding-bottom:5px;
}

QScrollBar::add-line:vertical {
    height: 13px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #484848, stop:1 #4B4B4B);
    image: url(:/icon/icon/arrow_down.png);
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    padding-top:5px;
    padding-bottom:5px;
}

QScrollBar::sub-line:vertical:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #484848, stop:1 #585858);
}

QScrollBar::add-line:vertical:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #484848, stop:1 #585858);
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: transparent;
}

QScrollBar:horizontal {
    height: 13px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #4B4B4B);
    margin: 0 13px 0 13px;
}

QScrollBar::handle:horizontal {
    height: 13px;
    min-width: 30px;
    border-radius: 4px;
    margin-top:2px;
    margin-bottom:2px;
    margin-left: 10px;
    margin-right: 10px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #AAAAAA, stop:1 #7B7B7B);
}

QScrollBar::handle:horizontal:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #9C9C9C, stop:1 #6D6D6D);
}

QScrollBar::sub-line:horizontal {
    width: 13px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #4B4B4B);
    image: url(:/icon/icon/arrow_left.png);
    subcontrol-position: left;
    subcontrol-origin: margin;
    padding-left:5px;
    padding-right:5px;
}

QScrollBar::add-line:horizontal {
    width: 13px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #484848, stop:1 #4B4B4B);
    image: url(:/icon/icon/arrow_right.png);
    subcontrol-position: right;
    subcontrol-origin: margin;
    padding-left:5px;
    padding-right:5px;
}

QScrollBar::sub-line:horizontal:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #585858, stop:1 #484848);
}

QScrollBar::add-line:horizontal:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #585858, stop:1 #484848);
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: transparent;
}

